{
  "name": "@kuzanatoliorg/testing-library-react-utils",
  "version": "1.5.2",
  "description": "The utils package for react testing library",
  "scripts": {
    "clean-artifacts": "node utils/clean.js",
    "build": "tsc",
    "build:local": "yarn run clean-artifacts && yarn run build",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "jest src --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzanatoliy/react-testing-library-utils.git"
  },
  "keywords": [
    "react",
    "react-testing-library",
    "tests",
    "jest"
  ],
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "author": "Anatoli Kuzmiankou <njkznu@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kuzanatoliy/react-testing-library-utils/issues"
  },
  "homepage": "https://github.com/kuzanatoliy/react-testing-library-utils#readme",
  "dependencies": {
    "@testing-library/react": ">=13.0.0",
    "@testing-library/react-hooks": ">=8.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
